{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": {
        "rewrites": [
          {
            "source": "/api/(.*)",
            "destination": "https://your-backend.onrender.com/api/$1"
          },
          {
            "source": "/(.*)",
            "destination": "/"
          }
        ]
      }
      
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
